{
  "name": "wml",
  "version": "0.1.1",
  "description": "Widget Markup Language",
  "main": "index.js",
  "scripts": {
    "test": "npm run -s test-parser && npm run -s test-core",
    "test-parser": "./node_modules/.bin/mocha --opts mocha.opts test/parse.js",
    "test-transpiler": "./node_modules/.bin/mocha --opts mocha.opts test/transpile.js",
    "test-core": "./node_modules/.bin/jest build",
    "build": "rm -R lib; mkdir -p lib; npm run parser; ./node_modules/.bin/babel -s inline -D src -d lib",
    "parser": "./node_modules/.bin/jison -o src/parse/ParserImpl.js src/wml.y",
    "api-docs": "./node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "author": "Lasana Murray <lmurray@quenk.com> (http://quenk.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^5.8.35",
    "browserify": "^13.0.0",
    "jison": "^0.4.16",
    "mocha": "^2.3.4",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "jest-cli": "^0.8.2",
    "babel-jest": "^5.3.0",
    "must": "^0.13.1"
  },
  "dependencies": {},
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest/index.js",
    "unmockedModulePathPatterns": [
      "build/*",
      "src/*"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}
