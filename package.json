{
  "name": "@quenk/wml",
  "version": "0.7.0",
  "description": "Widget Markup Language",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run -s test-parse",
    "test-parse": "./node_modules/.bin/mocha --opts mocha.opts test/parse.js",
    "test-compile": "./node_modules/.bin/mocha --opts mocha.opts test/compile.js",
    "test-runtime": "./node_modules/.bin/jest lib",
    "build": "rm -R lib; mkdir -p lib; npm run parser; ./node_modules/.bin/babel -s inline -D src -d lib",
    "parser": "./node_modules/.bin/jison -o src/parser/ParserImpl.js src/wml.y",
    "api-docs": "./node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "author": "Lasana Murray <lmurray@quenk.com> (http://quenk.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "browserify": "^13.0.0",
    "jest-cli": "^0.8.2",
    "jison": "^0.4.16",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "mocha": "^2.3.4",
    "must": "^0.13.1"
  },
  "dependencies": {
    "@quenk/wml-widgets": "^0.11.5",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "js-beautify": "^1.6.14",
    "property-seek": "^0.1.0",
    "through": "^2.3.8"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest/index.js",
    "unmockedModulePathPatterns": [
      "build/*",
      "src/*"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}
